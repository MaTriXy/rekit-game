package edu.kit.informatik.ragnarok.logic;

public class Vec2D {
	private float x;

	private void setX(float value) {
		this.x = value;
	}

	public float getX() {
		return this.x;
	}

	private float y;

	private void setY(float value) {
		this.y = value;
	}

	public float getY() {
		return this.y;
	}
	
	public Vec2D(float x, float y) {
		this.setX(x);
		this.setY(y);
	}

}
